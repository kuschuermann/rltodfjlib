include-etc=false
checksum-executable=true
git-support=true
svn-support=false
create-skeleton=false
sign-executable=false
sign-libraries=false
sign-main-jar=false
sign-properties-file=
jaxb-support=false
path-src=src
path-lib=lib
path-img=img
path-etc=etc
path-build=.build

